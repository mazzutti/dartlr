dfaClass(dfa) ::= <<
class DFA_<grammar.recognizerName>_<dfa.decisionNumber> extends DFA {

  DFA_<grammar.recognizerName>_<dfa.decisionNumber>(BaseRecognizer recognizer) : super(recognizer) {        
    decisionNumber = <dfa.decisionNumber>;
    eot = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_eot;
    eof = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_eof;
    min_ = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_min;
    max_ = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_max;
    accept = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_accept;
    special = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_special;
    transition = <grammar.recognizerName>.DFA_<grammar.recognizerName>_<dfa.decisionNumber>_transition;
  }
    
  String get description => 
        """<dfa.description>""";
  
  <@errorMethod()>
<if(dfa.specialStateSTs)>
  int specialStateTransition(int s, IntStream _input){
    <if(LEXER)>
    IntStream input = _input;
    <endif>
    <if(PARSER)>
    TokenStream input = _input;
    <endif>
    <if(TREE_PARSER)>
    TreeNodeStream input = _input;
    <endif>
    int _s = s;
    switch(s) {
      <dfa.specialStateSTs:{state |case <i0> :<state>}; separator="\n">
    }
<if(backtracking)>
    if(recognizer.state.backtracking > 0) {
      recognizer.state.failed = true; 
      return -1;
    }
<endif>
    NoViableAltException nvae = new NoViableAltException(description, <dfa.decisionNumber>, _s, recognizer.input);
    recognizer.reportError(nvae);
    throw nvae;
  }<\n>
<endif>
}<\n>
>>


